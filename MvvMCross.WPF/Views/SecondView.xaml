<views:MvxWpfView x:Class="MvvMCross.WPF.Views.SecondView"
             xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MvvMCross.WPF.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label VerticalAlignment="Center" HorizontalAlignment="Center" Content="Welcome to the SecondView" Margin="0 0 0 100"></Label>
        <Button Content="Back to FirstView" HorizontalAlignment="Center" VerticalAlignment="Center" Width="175" Command="{Binding NavigateToFirstViewCommand}"/>
    </Grid>
</views:MvxWpfView>
